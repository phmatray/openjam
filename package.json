{
  "name": "client",
  "version": "0.2.6",
  "engines": {
    "node": "10.9.0"
  },
  "private": true,
  "homepage": "https://beta.openjam.eu",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.8",
    "@fortawesome/free-brands-svg-icons": "^5.5.0",
    "@fortawesome/free-solid-svg-icons": "^5.5.0",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "acorn": "^6.0.2",
    "ajv": "^6.5.4",
    "axios": "^0.18.0",
    "husky": "^1.1.3",
    "i18next": "^12.0.0",
    "jwt-decode": "^2.2.0",
    "lint-staged": "^8.0.4",
    "moment": "^2.22.2",
    "popmotion": "8.4.1",
    "prop-types": "^15.6.2",
    "react": "^16.6.0",
    "react-blur": "^0.6.0",
    "react-dom": "^16.6.0",
    "react-https-redirect": "^1.0.11",
    "react-i18next": "^8.3.5",
    "react-kawaii": "^0.6.0",
    "react-loadable": "^5.5.0",
    "react-markdown": "^4.0.3",
    "react-moment": "^0.8.3",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.1",
    "react-sound": "^1.2.0",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "semantic-ui-react": "^0.83.0",
    "source-map-explorer": "^1.6.0",
    "styled-components": "^4.0.3"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "scripts": {
    "dev": "npm run build:semantic && npm start",
    "dev:https": "HTTPS=true npm start",
    "start": "react-scripts start",
    "start:storybook": "start-storybook -p 9001 -c .storybook",
    "build": "npm run build:semantic && react-scripts build",
    "build:storybook": "build-storybook -s public",
    "build:semantic": "cd src/semantic && gulp build-css build-assets",
    "watch:semantic": "cd src/semantic && npm run build:semantic && gulp watch",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "test": "react-scripts test --env=jsdom",
    "test:debug": "react-scripts --inspect-brk test --runInBand",
    "coverage": "react-scripts test --env=jsdom --coverage",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
    "analyze": "source-map-explorer build/static/js/main.*"
  },
  "devDependencies": {
    "@storybook/addon-actions": "4.0.0",
    "@storybook/addon-links": "4.0.0",
    "@storybook/addons": "4.0.0",
    "@storybook/react": "4.0.0",
    "babel-eslint": "^9.0.0",
    "coveralls": "^3.0.2",
    "eslint": "5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-babel": "^8.0.1",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "gulp": "^3.9.1",
    "gulp-help": "^1.6.1",
    "prettier": "^1.14.3",
    "semantic-ui": "^2.4.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
